#include<stdio.h>
main()
{
	int n;
	int sum=0;
	printf("enter a numb");
	scanf("%d",&n);
	int k=n;
	int r;
	while(k!=0)
	{
		r=k%10;
		int f=fact(r);
		k=k/10;
		sum=sum+f;
	}
	if(sum==n)
	{
		printf("/n numb is strong");
	}
	else
	{
		printf("/n numb is not strong");
	}
	return 0;
}
int fact(int r)
{
	int mul=1;
	for(int i=1;i<=r;i++)
	{
		mul=mul*i;
	}
	returnÂ mul;
}
